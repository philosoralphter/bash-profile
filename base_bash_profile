#Bash-profile Repo Location
# e.g.: ~/dropbox/env-settings/bash-profile/src-this
# and ~/dropbox/env-settings/bash-profile/private/src-this
IMPORT_DIR=
PRIVATE_DIR=

#ENV VARS
#JAVA_HOME=
#export GOPATH=




##########
#Imports
#Common Utils
##########

#Load Git Repo Data in Prompt
. $IMPORT_DIR/git-prompt-setup.sh
. $IMPORT_DIR/git-aliases.sh
source $IMPORT_DIR/prompt-command.sh

#Util functions
source $IMPORT_DIR/util-functions.sh

#Aliases
source $IMPORT_DIR/aliases.sh

#Chrome cli
source  $IMPORT_DIR/CLIs.sh

#Setup Docker
#source $IMPORT_DIR/docker-setup

#GoLang
source $IMPORT_DIR/golang.sh

#Editors
source $IMPORT_DIR/editor-settings.sh

#Shell Utils
source $IMPORT_DIR/shell-utils.sh

#AWS
source $IMPORT_DIR/shell-utils.sh

#GCLOUD
source $IMPORT_DIR/google-cloud-sdk.sh


####Private
if [ -e $IMPORT_DIR/../private ]
then
        #AWS and other keys
        source $PRIVATE_DIR/src-this/keys.sh

        #Work
        for f in $PRIVATE_DIR/work/*.sh; do source $f; done

        #etc: directory change aliases, Keys...
        for f in $PRIVATE_DIR/src-this/*.sh; do source $f; done
fi

